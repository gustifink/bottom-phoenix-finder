{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python@3.0.0",
      "config": { "pythonVersion": "3.9" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "check": true 
    }
  ]
} 